package com.shinetech.dress.tag;

public class QcBean {
	private float score;
	private int pcount;
	private float benefit;

	public float getScore() {
		return score;
	}

	public void setScore(float score, float ratio, int pageSize) {
		this.score += ((score * ratio) / pageSize);
	}

	public void setBenefit(float benefit, float ratio, int pageSize) {
		this.benefit += ((benefit * ratio) / pageSize);
	}

	public int getPcount() {
		return pcount;
	}

	public void setPcount(int pcount) {
		this.pcount = pcount;
	}

	public float getBenefit() {
		return benefit;
	}

}
