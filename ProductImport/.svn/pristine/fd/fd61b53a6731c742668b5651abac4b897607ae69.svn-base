package com.shinetech.order.bean;

/**
 * Class description goes here.
 *
 * @author <a href="mailto:wushexin@gmail.com">Frank.Wu</a>
 * @version 1.0.0
 */

public class OrderImgBean {
	private Long prodctId;
	private int pictureId;
	private String localPath;
	private String remotePath;
	private int isMain;
	/*
	 * @return the prodctId
	 */
	public Long getProdctId() {
		return prodctId;
	}
	/*
	 * @return the pictureId
	 */
	public int getPictureId() {
		return pictureId;
	}
	/*
	 * @return the localPath
	 */
	public String getLocalPath() {
		return localPath;
	}
	/*
	 * @return the remotePath
	 */
	public String getRemotePath() {
		return remotePath;
	}
	/*
	 * @return the isMain
	 */
	public int getIsMain() {
		return isMain;
	}
	/*
	 * @param prodctId the prodctId to set
	 */
	public void setProdctId(Long prodctId) {
		this.prodctId = prodctId;
	}
	/*
	 * @param pictureId the pictureId to set
	 */
	public void setPictureId(int pictureId) {
		this.pictureId = pictureId;
	}
	/*
	 * @param localPath the localPath to set
	 */
	public void setLocalPath(String localPath) {
		this.localPath = localPath;
	}
	/*
	 * @param remotePath the remotePath to set
	 */
	public void setRemotePath(String remotePath) {
		this.remotePath = remotePath;
	}
	/*
	 * @param isMain the isMain to set
	 */
	public void setIsMain(int isMain) {
		this.isMain = isMain;
	}
	
}
